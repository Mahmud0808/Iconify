<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="270"
                android:endColor="@color/neumorph_outline_end"
                android:startColor="@color/neumorph_outline_start"
                android:type="linear" />
            <corners android:radius="24.0dp" />
        </shape>
    </item>
    <item
        android:bottom="3.0dp"
        android:end="3.0dp"
        android:start="3.0dp"
        android:top="3.0dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="@color/neumorph_outline_end"
                android:startColor="@color/neumorph_outline_start"
                android:type="linear" />
            <corners android:radius="21.0dp" />
        </shape>
    </item>
</layer-list>
