<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="0.0"
                android:endColor="@color/holo_green_light"
                android:startColor="@color/holo_blue_light" />
            <corners android:radius="24dp" />
        </shape>
    </item>
    <item
        android:width="14dp"
        android:height="14dp"
        android:drawable="@drawable/qs_shape_bookmark_arrow_enabled_bg"
        android:gravity="start|center"
        android:start="43dp" />
    <item
        android:width="14dp"
        android:height="14dp"
        android:drawable="@drawable/qs_shape_bookmark_arrow_fg"
        android:gravity="start|center"
        android:start="43dp" />
    <item android:gravity="start|center">
        <shape android:shape="rectangle">
            <gradient
                android:angle="0.0"
                android:endColor="@color/holo_blue_light"
                android:startColor="@color/holo_green_light" />
            <corners
                android:bottomRightRadius="24dp"
                android:topRightRadius="24dp" />
            <size
                android:width="50dp"
                android:height="60dp" />
        </shape>
    </item>
    <item android:gravity="start|center">
        <shape android:shape="rectangle">
            <solid android:color="@color/offStateColor3" />
            <corners
                android:bottomRightRadius="24dp"
                android:topRightRadius="24dp" />
            <size
                android:width="50dp"
                android:height="60dp" />
        </shape>
    </item>
</layer-list>