<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PreferenceMenu">
        <attr name="showArrow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SwitchPreference">
        <attr name="requiresSystemUiRestart" format="boolean" />
        <attr name="requiresDeviceRestart" format="boolean" />
        <attr name="requiresThemeSwitch" format="boolean" />
        <attr name="requiresDeviceRotation" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ListPreference">
        <attr name="requiresSystemUiRestart" />
        <attr name="requiresDeviceRestart" />
        <attr name="requiresThemeSwitch" />
        <attr name="requiresDeviceRotation" />
    </declare-styleable>

    <declare-styleable name="ColorPreference">
        <attr name="requiresSystemUiRestart" />
        <attr name="requiresDeviceRestart" />
        <attr name="requiresThemeSwitch" />
        <attr name="requiresDeviceRotation" />
    </declare-styleable>

    <declare-styleable name="SearchPreference">
        <attr name="textHint" format="string" />
        <attr name="textNoResults" format="string" />
        <attr name="textClearHistory" format="string" />
    </declare-styleable>

    <declare-styleable name="SliderPreference">
        <attr name="valueCount" format="integer" />
        <attr name="updatesContinuously" format="boolean" />
        <attr name="tickInterval" format="float" />
        <attr name="minVal" format="float" />
        <attr name="maxVal" format="float" />
        <attr name="showResetButton" format="boolean" />
        <attr name="showValueLabel" format="boolean" />
        <attr name="units" format="string" />
        <attr name="requiresSystemUiRestart" />
        <attr name="requiresDeviceRestart" />
        <attr name="requiresThemeSwitch" />
        <attr name="requiresDeviceRotation" />
    </declare-styleable>

    <declare-styleable name="footerPreferenceStyle">
        <attr name="footerPreferenceStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="BackgroundChipPreference">
        <attr name="backgroundChipStyle" format="integer" />
    </declare-styleable>

    <declare-styleable name="MaterialTimePickerPreference">
        <attr name="presetValue" format="string" />
    </declare-styleable>

    <declare-styleable name="FadeTransition">
        <attr name="startAlpha" format="float" />
        <attr name="endAlpha" format="float" />
    </declare-styleable>

    <declare-styleable name="PopTransition">
        <attr name="startScale" format="float" />
        <attr name="endScale" format="float" />
    </declare-styleable>

    <declare-styleable name="BounceScrollView">
        <attr name="damping" format="float" />
        <attr name="scrollOrientation" format="enum">
            <enum name="vertical" value="0" />
            <enum name="horizontal" value="1" />
        </attr>
        <attr name="incrementalDamping" format="boolean" />
        <attr name="bounceDelay" format="integer" />
        <attr name="triggerOverScrollThreshold" format="integer" />
    </declare-styleable>

    <declare-styleable name="TitleWidget">
        <attr name="titleText" format="string" />
        <attr name="summaryText" format="string" />
        <attr name="iconSpaceReserved" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MenuWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="icon" format="reference" />
        <attr name="iconSpaceReserved" />
        <attr name="showEndArrow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SwitchWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="icon" />
        <attr name="iconSpaceReserved" />
        <attr name="isChecked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SliderWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="sliderStepSize" format="integer" />
        <attr name="sliderValue" format="integer" />
        <attr name="sliderValueFrom" format="integer" />
        <attr name="sliderValueTo" format="integer" />
        <attr name="valueFormat" format="string" />
        <attr name="sliderDefaultValue" format="integer" />
        <attr name="isDecimalFormat" format="boolean" />
        <attr name="decimalFormat" format="string" />
        <attr name="outputScale" format="float" />
        <attr name="hasResetButton" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ColorPickerWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="previewColor" format="reference" />
    </declare-styleable>

    <declare-styleable name="ColorPickerSmallWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="previewColor" />
    </declare-styleable>

    <declare-styleable name="FilePickerWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="buttonText" format="string" />
        <attr name="filePickerType" format="string" />
    </declare-styleable>

    <declare-styleable name="RadioDialogWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="icon" />
        <attr name="iconSpaceReserved" />
        <attr name="entries" format="reference" />
        <attr name="showSelectedPrefix" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BottomSheetWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="icon" />
        <attr name="iconSpaceReserved" />
        <attr name="showSelectedPrefix" />
        <attr name="entries" />
    </declare-styleable>

    <declare-styleable name="EditTextWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="dialogTitleText" format="string" />
        <attr name="dialogSubtitleText" format="string" />
        <attr name="dialogHintText" format="string" />
        <attr name="dialogText" format="string" />
    </declare-styleable>

    <declare-styleable name="CategoryWidget">
        <attr name="titleText" />
    </declare-styleable>

    <declare-styleable name="ListWidget">
        <attr name="titleText" />
        <attr name="summaryText" />
        <attr name="icon" />
        <attr name="iconSpaceReserved" />
        <attr name="entries" />
        <attr name="entryValues" format="reference" />
    </declare-styleable>

</resources>