<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <layer-list>
            <item>
                <shape
                    android:shape="rectangle"
                    android:tintMode="src_atop">
                    <solid android:color="@*android:color/system_neutral2_800" />
                    <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
                </shape>
            </item>
            <item>
                <shape
                    android:shape="rectangle"
                    android:tintMode="src_atop">
                    <solid android:color="#1AFFFFFF" />
                    <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
                </shape>
            </item>
            <item
                android:bottom="3dp"
                android:end="40dp"
                android:top="6dp">
                <shape
                    android:shape="rectangle"
                    android:tintMode="src_atop">
                    <solid android:color="@*android:color/system_neutral2_800" />
                    <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
                </shape>
            </item>
            <item
                android:bottom="6dp"
                android:start="40dp"
                android:top="3dp">
                <shape
                    android:shape="rectangle"
                    android:tintMode="src_atop">
                    <solid android:color="@*android:color/system_neutral2_800" />
                    <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape
                    android:shape="rectangle"
                    android:tintMode="multiply">
                    <gradient
                        android:angle="315.0"
                        android:endColor="@*android:color/holo_green_light"
                        android:startColor="@*android:color/holo_blue_light" />
                    <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
                </shape>
            </item>
            <item
                android:bottom="3dp"
                android:end="40dp"
                android:top="6dp">
                <shape
                    android:shape="rectangle"
                    android:tintMode="multiply">
                    <solid android:color="@*android:color/system_neutral2_800" />
                    <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
                </shape>
            </item>
            <item
                android:bottom="6dp"
                android:start="40dp"
                android:top="3dp">
                <shape
                    android:shape="rectangle"
                    android:tintMode="multiply">
                    <solid android:color="@*android:color/system_neutral2_800" />
                    <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
                </shape>
            </item>
        </layer-list>
    </item>
</layer-list>
