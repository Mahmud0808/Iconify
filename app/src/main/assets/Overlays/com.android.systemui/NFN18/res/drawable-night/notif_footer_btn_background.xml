<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@*android:color/black" />
            <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
            <padding
                android:left="20dp"
                android:right="20dp" />
            <stroke
                android:width="2dp"
                android:color="#1AFFFFFF" />
        </shape>
    </item>
</layer-list>
