<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="315.0"
                android:endColor="@*android:color/holo_green_light"
                android:startColor="@*android:color/holo_blue_light" />
            <corners android:radius="@*android:dimen/harmful_app_name_padding_top" />
        </shape>
    </item>
    <item
        android:bottom="4.0dip"
        android:end="4.0dip"
        android:start="4.0dip"
        android:top="4.0dip">
        <shape android:shape="rectangle">
            <solid android:color="?android:attr/colorBackgroundFloating" />
            <corners android:radius="@*android:dimen/harmful_app_name_padding_left" />
        </shape>
    </item>
</layer-list>
