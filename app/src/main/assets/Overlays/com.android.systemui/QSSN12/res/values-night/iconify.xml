<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.SystemUI.QuickSettings" parent="@*android:style/Theme.DeviceDefault.SystemUI">
        <item name="android:colorPrimary">@*android:color/holo_green_light</item>
        <item name="android:colorSecondary">@*android:color/holo_green_dark</item>
        <item name="android:colorBackground">@*android:color/system_surface_bright_dark</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:itemTextAppearance">@style/Control.MenuItem</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_arrow_back</item>
        <item name="android:colorError">@*android:color/error_color_device_default_dark</item>
        <item name="darkIconTheme">@style/QSIconTheme</item>
        <item name="isQsTheme">true</item>
        <item name="lightIconTheme">@style/QSIconTheme</item>
        <item name="onShadeActive">@*android:color/system_accent1_900</item>
        <item name="onShadeActiveVariant">@*android:color/system_accent1_700</item>
        <item name="onShadeInactive">@*android:color/system_neutral1_100</item>
        <item name="onShadeInactiveVariant">@*android:color/system_neutral2_200</item>
        <item name="onSurface">@*android:color/system_on_surface_dark</item>
        <item name="onSurfaceVariant">@*android:color/system_on_surface_variant_dark</item>
        <item name="outline">@*android:color/system_outline_dark</item>
        <item name="primary">@*android:color/system_primary_dark</item>
        <item name="scHigh">@*android:color/system_surface_container_high_dark</item>
        <item name="shadeActive">#FFFFFFFF</item>
        <item name="shadeDisabled">@color/shade_disabled</item>
        <item name="shadeInactive">@*android:color/system_neutral2_800</item>
        <item name="surfaceBright">@*android:color/system_surface_bright_dark</item>
        <item name="tertiary">@*android:color/system_tertiary_dark</item>
        <item name="underSurface">@*android:color/system_neutral1_1000</item>
    </style>
    <style name="Control" />
    <style name="Control.MenuItem" parent="@style/Control">
        <item name="android:textSize">@dimen/control_menu_item_text_size</item>
        <item name="android:textColor">@color/control_primary_text</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:minHeight">@dimen/control_menu_item_height</item>
        <item name="android:singleLine">true</item>
        <item name="android:fontFamily">@*android:string/config_bodyFontFamily</item>
    </style>
    <style name="QSIconTheme">
        <item name="backgroundColor">?android:textColorHint</item>
        <item name="fillColor">?android:textColorPrimary</item>
        <item name="singleToneColor">?android:textColorPrimary</item>
    </style>
    <dimen name="control_menu_item_text_size">16.0sp</dimen>
    <dimen name="control_menu_item_height">54.0dip</dimen>
    <color name="control_primary_text">#e6ffffff</color>
    <dimen name="qs_quick_tile_size">60.0dp</dimen>
    <dimen name="qs_tile_start_padding">20.0dp</dimen>
    <dimen name="qs_label_container_margin">24.0dp</dimen>
</resources>
