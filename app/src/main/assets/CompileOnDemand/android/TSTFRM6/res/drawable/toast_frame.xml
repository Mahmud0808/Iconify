                android:bottomLeftRadius="@*android:dimen/harmful_app_name_padding_top"
                android:bottomRightRadius="@*android:dimen/harmful_app_name_padding_top"
                android:topLeftRadius="@*android:dimen/harmful_app_name_padding_left"
                android:topRightRadius="@*android:dimen/harmful_app_name_padding_left" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:end="12dp"
        android:start="12dp">
        <shape android:shape="rectangle">
            <gradient
                android:angle="0.0"
                android:endColor="@*android:color/holo_green_light"
                android:startColor="@*android:color/holo_blue_light" />
            <corners
                android:bottomLeftRadius="@*android:dimen/harmful_app_name_padding_top"
                android:bottomRightRadius="@*android:dimen/harmful_app_name_padding_top"
