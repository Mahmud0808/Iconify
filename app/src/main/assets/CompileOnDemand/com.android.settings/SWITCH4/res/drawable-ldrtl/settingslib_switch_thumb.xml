<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item
                android:bottom="2dp"
                android:right="-4dp"
                android:left="2dp"
                android:top="2dp">
                <shape android:shape="rectangle">
                    <solid android:color="#FFFFFFFF" />
                    <corners android:radius="26dp" />
                    <size
                        android:width="26dp"
                        android:height="26dp" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item
                android:bottom="2dp"
                android:right="2dp"
                android:left="-4dp"
                android:top="2dp">
                <shape android:shape="rectangle">
                    <solid android:color="#FFFFFFFF" />
                    <corners android:radius="26dp" />
                    <size
                        android:width="26dp"
                        android:height="26dp" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
